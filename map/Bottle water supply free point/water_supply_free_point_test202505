<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>GeoJSON Map</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://unpkg.com/leaflet/dist/leaflet.css" />
  <style>
    #map { height: 100vh; }
  </style>
</head>
<body>
  <div id="map"></div>
  <script src="https://unpkg.com/leaflet/dist/leaflet.js"></script>
  <script>
    const map = L.map('map').setView([33.0, 130.0], 10); // 適宜中心を調整
    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
      attribution: '© OpenStreetMap contributors'
    }).addTo(map);

    fetch('https://raw.githubusercontent.com/GX-87/push_work/main/map/Bottle%20water%20supply%20free%20point/Bottle%20water%20supply%20free%20point_test1.geojson')
      .then(response => response.json())
      .then(data => {
        L.geoJSON(data).addTo(map);
      })
      .catch(err => console.error(err));
  </script>
</body>
</html>
